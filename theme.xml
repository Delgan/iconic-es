<?xml version="1.0" encoding="UTF-8"?>
<!--
Theme Name: Iconic (ES version)
Version:    0.0.1
Author:     Delgan
License:    CC0 1.0
Based on:   Iconic (ES-DE version) by Siddy212
-->
<theme defaultTransition="instant">
    <formatVersion>7</formatVersion>
    <subset name="color-scheme" displayName="Color Scheme">
        <include name="light" displayName="Light" />
        <include name="dark" displayName="Dark" />
    </subset>
    <subset name="metadata-source" displayName="Metadata Source">
        <include name="theme" displayName="Theme" />
        <include name="es" displayName="EmulationStation" />
    </subset>
    <variables ifSubset="color-scheme:dark">
        <systemCarouselSelectorImage>system-carousel-selector-dark-with-shadow.png</systemCarouselSelectorImage>
        <bottomBarImage>bottom-bar-dark.png</bottomBarImage>
        <systemInfoColor>cccccc</systemInfoColor>
        <systemManufacturerColor>adadad</systemManufacturerColor>
        <systemInfoSecondaryColor>a0a0a0</systemInfoSecondaryColor>
        <helpTextColor>999999</helpTextColor>
        <helpIconColor>999999</helpIconColor>
        <menuBackgroundColor>151515</menuBackgroundColor>
        <menuTitleColor>cccccc</menuTitleColor>
        <menuGroupColor>cccccc</menuGroupColor>
        <menuTextColor>cccccc</menuTextColor>
        <menuFooterColor>a0a0a0</menuFooterColor>
        <menuSelectedColor>f0f0f0</menuSelectedColor>
        <menuSelectorColor>444444</menuSelectorColor>
        <menuTextSeparatorColor>44444430</menuTextSeparatorColor>
        <menuGroupSeparatorColor>444444</menuGroupSeparatorColor>
        <gamelistBackgroundImage>gamelist-background-dark-medium.svg</gamelistBackgroundImage>
        <gamelistBackgroundImageDetailed>gamelist-metadata-background-dark-extended.svg</gamelistBackgroundImageDetailed>
        <gamelistSelectorColor>444444</gamelistSelectorColor>
        <gamelistSelectedColor>f0f0f0</gamelistSelectedColor>
        <gamelistPrimaryColor>cccccc</gamelistPrimaryColor>
        <gamelistSecondaryColor>cccccc</gamelistSecondaryColor>
        <gameMetadataColor>cccccc</gameMetadataColor>
    </variables>
    <variables ifSubset="color-scheme:light">
        <systemCarouselSelectorImage>system-carousel-selector-light-with-shadow.png</systemCarouselSelectorImage>
        <bottomBarImage>bottom-bar-light.png</bottomBarImage>
        <systemInfoColor>555555</systemInfoColor>
        <systemManufacturerColor>6e6e6e</systemManufacturerColor>
        <systemInfoSecondaryColor>a0a0a0</systemInfoSecondaryColor>
        <helpTextColor>616161</helpTextColor>
        <helpIconColor>616161</helpIconColor>
        <menuBackgroundColor>F0F0F0</menuBackgroundColor>
        <menuTitleColor>555555</menuTitleColor>
        <menuGroupColor>555555</menuGroupColor>
        <menuTextColor>555555</menuTextColor>
        <menuFooterColor>a0a0a0</menuFooterColor>
        <menuSelectedColor>f0f0f0</menuSelectedColor>
        <menuSelectorColor>666666</menuSelectorColor>
        <menuTextSeparatorColor>6666661A</menuTextSeparatorColor>
        <menuGroupSeparatorColor>666666</menuGroupSeparatorColor>
        <gamelistBackgroundImage>gamelist-background-light-medium.svg</gamelistBackgroundImage>
        <gamelistBackgroundImageDetailed>gamelist-metadata-background-light-extended.svg</gamelistBackgroundImageDetailed>
        <gamelistSelectorColor>666666</gamelistSelectorColor>
        <gamelistSelectedColor>F0F0F0</gamelistSelectedColor>
        <gamelistPrimaryColor>555555</gamelistPrimaryColor>
        <gamelistSecondaryColor>555555</gamelistSecondaryColor>
        <gameMetadataColor>555555</gameMetadataColor>
    </variables>
    <!-- Set default description and cart size when none is available. -->
    <variables>
        <systemDescription>View and play ${system.fullName}.</systemDescription>
        <systemCoverSize>1-1</systemCoverSize>
        <systemCartSize>1-1</systemCartSize>
    </variables>
    <!-- Built-in metadata is used as fallback when source is "Theme". -->
    <include ifSubset="metadata-source:theme">./_inc/metadata/_builtin.xml</include>
    <!-- Load "Theme" metadata (required in "EmulationStation" mode to retrieve description). -->
    <include>./_inc/metadata/${system.theme}.xml</include>
    <!-- Built-in metadata override theme metadata when source is "EmulationStation". -->
    <include ifSubset="metadata-source:es">./_inc/metadata/_builtin.xml</include>
    <view name="screen">
        <controllerActivity name="controllerActivity">
            <visible>false</visible>
        </controllerActivity>
        <batteryIndicator name="batteryIndicator">
            <visible>false</visible>
        </batteryIndicator>
        <text name="clock">
            <pos>1 1</pos>
        </text>
    </view>
    <view name="system, basic, detailed">
        <stackpanel name="system-indicators" extra="true">
            <h>0.022</h>
            <pos>-0.0075 0.965</pos>
            <orientation>horizontal</orientation>
            <reverse>true</reverse>
            <clock name="system-clock">
                <color>${helpTextColor}</color>
                <fontSize>0.022</fontSize>
                <lineSpacing>1</lineSpacing>
                <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            </clock>
            <batteryText name="system-battery-text">
                <color>${helpTextColor}</color>
                <fontSize>0.022</fontSize>
                <lineSpacing>1</lineSpacing>
                <fontPath>./_inc/fonts/Gilroy-Regular.ttf</fontPath>
            </batteryText>
            <batteryIcon name="system-battery-icon">
                <maxSize>1 1</maxSize>
                <color>${helpIconColor}</color>
                <full>./_inc/other/battery-icon-100.svg</full>
                <at75>./_inc/other/battery-icon-75.svg</at75>
                <at50>./_inc/other/battery-icon-50.svg</at50>
                <at25>./_inc/other/battery-icon-25.svg</at25>
                <empty>./_inc/other/battery-icon-0.svg</empty>
                <incharge>./_inc/other/battery-icon-charging.svg</incharge>
            </batteryIcon>
            <networkIcon name="system-network-icon">
                <maxSize>1 1</maxSize>
                <color>${helpIconColor}</color>
                <networkIcon>./_inc/other/network-icon-on.svg</networkIcon>
                <planemodeIcon>./_inc/other/network-icon-off.svg</planemodeIcon>
            </networkIcon>
        </stackpanel>
    </view>
    <view name="system, basic, detailed">
        <stackpanel name="system-indicators" extra="true">
            <w>1</w>
            <separator>0.012</separator>
            <zIndex>100</zIndex>
        </stackpanel>
    </view>
    <view name="system">
        <image name="logo">
            <path>./_inc/system-logo/${system.theme}.webp</path>
        </image>
        <carousel name="systemcarousel">
            <type>horizontal</type>
            <origin>0 1</origin>
            <pos>0 1</pos>
            <size>1 0.2</size>
            <logoSize>0.2 0.2</logoSize>
            <color>00000000</color>
            <colorEnd>00000000</colorEnd>
            <gradientType>vertical</gradientType>
            <logoScale>1.6</logoScale>
            <minLogoOpacity>0.5</minLogoOpacity>
            <maxLogoCount>4</maxLogoCount>
            <logoPos>0.15 0</logoPos>
            <systemInfoDelay>0</systemInfoDelay>
            <defaultTransition>instant</defaultTransition>
            <itemTemplate>
                <container>
                    <image name="logo-image">
                        <pos>0.5 0.5</pos>
                        <origin>0.5 0.5</origin>
                        <maxSize>1 1</maxSize>
                        <linearSmooth>true</linearSmooth>
                        <path>{system:logo}</path>
                    </image>
                    <text name="logo-text-fallback">
                        <visible>!exists("${system:logo}")</visible>
                        <value>{system:fullName}</value>
                        <forceUppercase>true</forceUppercase>
                        <lineSpacing>1</lineSpacing>
                        <alignment>center</alignment>
                        <verticalAlignment>center</verticalAlignment>
                        <origin>0.5 0.5</origin>
                        <pos>0.5 0.5</pos>
                        <fontSize>0.04</fontSize>
                        <glowColor>fafafaff</glowColor>
                        <multiline>true</multiline>
                        <glowSize>2</glowSize>
                        <size>0.74 0.45</size>
                        <padding>0.04 0 0.04 0</padding>
                        <glowOffset>0 0</glowOffset>
                        <fontPath>./_inc/fonts/ChangaOne-Italic.ttf</fontPath>
                        <color>111111ff</color>
                    </text>
                </container>
            </itemTemplate>
        </carousel>
        <image name="background-art" extra="true">
            <default>./_inc/system-background-art/_default.webp</default>
            <path>./_inc/system-background-art/${system.theme}.webp</path>
            <size>1 1</size>
        </image>
        <image name="system-image" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.25 .23</pos>
            <size>.24 0</size>
            <default>./_inc/controllers/_default.webp</default>
            <path>./_inc/controllers/${system.theme}.webp</path>
            <zIndex>15</zIndex>
        </image>
        <image name="system-carousel-selector" extra="true">
            <origin>0 0</origin>
            <pos>0 0</pos>
            <size>1 1</size>
            <path>./_inc/other/${systemCarouselSelectorImage}</path>
            <zindex>2</zindex>
            <linearSmooth>true</linearSmooth>
        </image>
        <text name="system-name" extra="true">
            <text>${systemName}</text>
            <origin>0.5 0.5</origin>
            <pos>0.25 0.474</pos>
            <size>0.26 0.05</size>
            <autoScrollSpeed>25</autoScrollSpeed>
            <autoScrollDelay>0</autoScrollDelay>
            <singleLineScroll>true</singleLineScroll>
            <fontSize>0.036777777777778</fontSize>
            <alignment>center</alignment>
            <forceUppercase>true</forceUppercase>
            <color>54585a</color>
            <zIndex>15</zIndex>
            <color>${systemInfoColor}</color>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </text>
        <text name="system-manufacturer" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.25 0.506</pos>
            <fontSize>0.019</fontSize>
            <size>0.26 0.05</size>
            <autoScrollSpeed>25</autoScrollSpeed>
            <autoScrollDelay>0</autoScrollDelay>
            <singleLineScroll>true</singleLineScroll>
            <text>${systemManufacturer}</text>
            <alignment>center</alignment>
            <forceUppercase>true</forceUppercase>
            <color>${systemManufacturerColor}</color>
            <zIndex>15</zIndex>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </text>
        <text name="system-year-type" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.25 0.543</pos>
            <fontSize>0.024</fontSize>
            <size>0.26 0.05</size>
            <autoScrollSpeed>25</autoScrollSpeed>
            <autoScrollDelay>0</autoScrollDelay>
            <singleLineScroll>true</singleLineScroll>
            <text if="${systemReleaseYear} > 0">${systemReleaseYear} · ${systemHardwareType}</text>
            <text if="!(${systemReleaseYear} > 0)">${systemHardwareType}</text>
            <alignment>center</alignment>
            <forceUppercase>true</forceUppercase>
            <color>${systemInfoColor}</color>
            <zIndex>15</zIndex>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </text>
        <text name="systemInfo">
            <origin>0.5 0.5</origin>
            <pos>0.25 0.605</pos>
            <size>0.26 0.024</size>
            <autoScrollSpeed>25</autoScrollSpeed>
            <autoScrollDelay>0</autoScrollDelay>
            <singleLineScroll>true</singleLineScroll>
            <fontSize>0.024</fontSize>
            <color>${systemInfoSecondaryColor}</color>
            <alignment>center</alignment>
            <lineSpacing>1</lineSpacing>
            <forceUppercase>true</forceUppercase>
            <zIndex>15</zIndex>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </text>
        <text name="system-description" extra="true">
            <origin>0.5 0</origin>
            <pos>0.25 0.635</pos>
            <size>0.24 0.175</size>
            <fontSize>0.024</fontSize>
            <fontPath>./_inc/fonts/Oxygen-Regular.ttf</fontPath>
            <text>${systemDescription}</text>
            <verticalAlignment>top</verticalAlignment>
            <alignment>center</alignment>
            <autoScroll>vertical</autoScroll>
            <autoScrollSpeed>50</autoScrollSpeed>
            <autoScrollDelay>5000</autoScrollDelay>
            <color>${systemInfoColor}</color>
            <zIndex>15</zIndex>
        </text>
        <helpsystem name="help">
            <origin>0 0</origin>
            <pos>0.48 0.967</pos>
            <fontSize>0.022</fontSize>
            <textColor>${helpTextColor}</textColor>
            <iconColor>${helpIconColor}</iconColor>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </helpsystem>
    </view>
    <view name="menu">
        <menuBackground name="menubg">
            <color>${menuBackgroundColor}</color>
            <selectorColor>${menuSelectorColor}</selectorColor>
            <cornerSize>20 20</cornerSize>
        </menuBackground>
        <menuText name="menutitle">
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <fontSize>0.034</fontSize>
            <color>${menuTitleColor}</color>
        </menuText>
        <menuGroup name="menugroup">
            <fontPath>./_inc/fonts/Gilroy-Regular.ttf</fontPath>
            <fontSize>0.034</fontSize>
            <color>${menuGroupColor}</color>
            <separatorColor>${menuGroupSeparatorColor}</separatorColor>
            <backgroundColor>00000000</backgroundColor>
        </menuGroup>
        <menuText name="menutext">
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <fontSize>0.032</fontSize>
            <color>${menuTextColor}</color>
            <separatorColor>${menuTextSeparatorColor}</separatorColor>
            <selectedColor>${menuSelectedColor}</selectedColor>
            <selectorColor>${menuSelectorColor}</selectorColor>
        </menuText>
        <menuTextSmall name="menutextsmall">
            <fontPath>./_inc/fonts/Gilroy-Regular.ttf</fontPath>
            <fontSize>0.025</fontSize>
            <color>${menuTextColor}</color>
        </menuTextSmall>
        <menuText name="menufooter">
            <fontPath>./_inc/fonts/Gilroy-Regular.ttf</fontPath>
            <fontSize>0.024</fontSize>
            <color>${menuFooterColor}</color>
        </menuText>
        <menuTextEdit name="menutextedit">
            <inactive>./_inc/other/menu-textinput.png</inactive>
            <active>./_inc/other/menu-textinput-active.png</active>
        </menuTextEdit>
        <menuIcons name="menuicons">
            <iconSystem>./_inc/other/menu-icon-system.svg</iconSystem>
            <iconUpdates>./_inc/other/menu-icon-update.svg</iconUpdates>
            <iconControllers>./_inc/other/menu-icon-controller.svg</iconControllers>
            <iconGames>./_inc/other/menu-icon-games.svg</iconGames>
            <iconUI>./_inc/other/menu-icon-ui.svg</iconUI>
            <iconSound>./_inc/other/menu-icon-sound.svg</iconSound>
            <iconNetwork>./_inc/other/menu-icon-network.svg</iconNetwork>
            <iconScraper>./_inc/other/menu-icon-scraper.svg</iconScraper>
            <iconAdvanced>./_inc/other/menu-icon-advanced.svg</iconAdvanced>
            <iconQuit>./_inc/other/menu-icon-quit.svg</iconQuit>
            <iconRetroachievements>./_inc/other/menu-icon-achievements.svg</iconRetroachievements>
            <iconKodi>./_inc/other/menu-icon-kodi.svg</iconKodi>
            <iconRestart>./_inc/other/menu-icon-restart.svg</iconRestart>
            <iconShutdown>./_inc/other/menu-icon-shutdown.svg</iconShutdown>
            <iconFastShutdown>./_inc/other/menu-icon-shutdown.svg</iconFastShutdown>
        </menuIcons>
    </view>
    <view name="basic">
        <image name="background-art" extra="true">
            <default>./_inc/system-background-art/_default.webp</default>
            <path>./_inc/system-background-art/${system.theme}.webp</path>
            <size>1 1</size>
            <saturation>0.65</saturation>
            <zIndex>0</zIndex>
        </image>
        <image name="gamelist-bottom-bar" extra="true">
            <origin>0 0</origin>
            <pos>0 0</pos>
            <size>1 1</size>
            <path>./_inc/other/${bottomBarImage}</path>
            <zindex>2</zindex>
            <linearSmooth>true</linearSmooth>
        </image>
        <image name="logo" origin="0.5 0.5" linearSmooth="true">
            <path>./_inc/system-logo/${system.theme}.webp</path>
            <zIndex>11</zIndex>
            <visible>true</visible>
            <pos>0.255 0.5</pos>
            <maxSize>0.5 .5</maxSize>
        </image>
        <image name="game-list-background-shadow" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.75 0.51</pos>
            <size>0.4453125 0.8972222528</size>
            <zIndex>11</zIndex>
            <linearSmooth>true</linearSmooth>
            <path>./_inc/other/gamelist-background-shadow.png</path>
        </image>
        <image name="game-list-background" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.75 0.51</pos>
            <size>0.4140625 0.84166666666</size>
            <zIndex>12</zIndex>
            <linearSmooth>true</linearSmooth>
            <path>./_inc/other/${gamelistBackgroundImage}</path>
        </image>
        <textlist name="gamelist">
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <selectorColor>${gamelistSelectorColor}</selectorColor>
            <selectedColor>${gamelistSelectedColor}</selectedColor>
            <primaryColor>${gamelistPrimaryColor}</primaryColor>
            <secondaryColor>${gamelistSecondaryColor}</secondaryColor>
            <horizontalMargin>0.0083</horizontalMargin>
            <zIndex>13</zIndex>
            <pos>0.57491666666667 0.134666666666667</pos>
            <size>0.348958333333333 0.7519</size>
            <fontSize>0.030333333333333</fontSize>
            <lines>13</lines>
            <alignment>left</alignment>
            <lineSpacing>1.2</lineSpacing>
        </textlist>
        <helpsystem name="help">
            <origin>0 0</origin>
            <pos>0.01 0.967</pos>
            <fontSize>0.022</fontSize>
            <textColor>${helpTextColor}</textColor>
            <iconColor>${helpIconColor}</iconColor>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
        </helpsystem>
    </view>
    <view name="detailed">
        <image name="background-art" extra="true">
            <default>./_inc/system-background-art/_default.webp</default>
            <path>./_inc/system-background-art/${system.theme}.webp</path>
            <size>1 1</size>
            <saturation>0.65</saturation>
            <zIndex>0</zIndex>
        </image>
        <image name="gamelist-bottom-bar" extra="true">
            <origin>0 0</origin>
            <pos>0 0</pos>
            <size>1 1</size>
            <path>./_inc/other/${bottomBarImage}</path>
            <zindex>2</zindex>
            <linearSmooth>true</linearSmooth>
        </image>
        <image name="md_marquee">
            <pos>0.37 0.10</pos>
            <maxSize>0.21 0.16</maxSize>
        </image>
        <image name="md_image">
            <pos>0.25 0.22</pos>
            <zIndex>11</zIndex>
            <maxSize>0.26 0.26</maxSize>
        </image>
        <image name="md_thumbnail">
            <pos>0.1 0.33</pos>
            <maxSize>0.14 0.16</maxSize>
        </image>
        <image name="logo" origin="0.5 0.5" linearSmooth="true" extra="true">
            <path>./_inc/system-logo/${system.theme}.webp</path>
            <pos>0.255 0.88</pos>
            <maxSize>0.33 .33</maxSize>
            <saturation>0.65</saturation>
            <opacity>0.4</opacity>
            <zIndex>11</zIndex>
        </image>
        <image name="game-metadata-background-image-extended" extra="true">
            <origin>0.5 0.5</origin>
            <path>./_inc/other/${gamelistBackgroundImageDetailed}</path>
            <pos>0.272 0.61</pos>
            <maxSize>0.5 .5</maxSize>
            <zIndex>11</zIndex>
        </image>
        <image name="game-list-background-shadow" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.75 0.51</pos>
            <size>0.4453125 0.8972222528</size>
            <zIndex>11</zIndex>
            <linearSmooth>true</linearSmooth>
            <path>./_inc/other/gamelist-background-shadow.png</path>
        </image>
        <image name="game-list-background" extra="true">
            <origin>0.5 0.5</origin>
            <pos>0.75 0.51</pos>
            <size>0.4140625 0.84166666666</size>
            <zIndex>12</zIndex>
            <linearSmooth>true</linearSmooth>
            <path>./_inc/other/${gamelistBackgroundImage}</path>
        </image>
        <text name="md_name">
            <origin>0 0</origin>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <color>${gameMetadataColor}</color>
            <lineSpacing>1</lineSpacing>
            <value>Unknown</value>
            <fontSize>0.024</fontSize>
            <autoScroll>horizontal</autoScroll>
            <autoScrollSpeed>50</autoScrollSpeed>
            <autoScrollDelay>1000</autoScrollDelay>
            <pos>0.05 0.437</pos>
            <size>0.46 0.024</size>
            <fontSize>0.024</fontSize>
            <lineSpacing>1</lineSpacing>
            <alignment>left</alignment>
        </text>
        <rating name="md_rating">
            <origin>0.5 0.5</origin>
            <filledPath>./_inc/other/icon-star-filled.svg</filledPath>
            <unfilledPath>./_inc/other/icon-star-unfilled.svg</unfilledPath>
            <color>${gameMetadataColor}</color>
            <pos>0.085 0.506</pos>
            <size>0 0.03</size>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <fontSize>0.024</fontSize>
            <lineSpacing>1</lineSpacing>
            <alignment>left</alignment>
        </rating>
        <text name="md_genre">
            <value>Unknown</value>
            <forceUppercase>true</forceUppercase>
            <origin>0 0.5</origin>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <filledPath>./_inc/other/icon-star-filled.svg</filledPath>
            <unfilledPath>./_inc/other/icon-star-unfilled.svg</unfilledPath>
            <color>${gameMetadataColor}</color>
            <lineSpacing>1</lineSpacing>
            <pos>0.188 0.507</pos>
            <size>0.32 0.024</size>
            <fontSize>0.024</fontSize>
            <alignment>left</alignment>
        </text>
        <datetime name="md_releasedate">
            <value>Unknown</value>
            <origin>0 0.5</origin>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <filledPath>./_inc/other/icon-star-filled.svg</filledPath>
            <unfilledPath>./_inc/other/icon-star-unfilled.svg</unfilledPath>
            <color>${gameMetadataColor}</color>
            <lineSpacing>1</lineSpacing>
            <fontSize>0.024</fontSize>
            <pos>0.05 0.564</pos>
            <size>0.09 0.024</size>
            <format>%m/%Y</format>
            <alignment>left</alignment>
        </datetime>
        <text name="md_developer">
            <value>Unknown</value>
            <origin>0 0.5</origin>
            <fontPath>./_inc/fonts/Gilroy-Bold.ttf</fontPath>
            <filledPath>./_inc/other/icon-star-filled.svg</filledPath>
            <unfilledPath>./_inc/other/icon-star-unfilled.svg</unfilledPath>
            <color>${gameMetadataColor}</color>
            <lineSpacing>1</lineSpacing>
            <pos>0.188 0.564</pos>
            <size>0.18 0.024</size>
            <fontSize>0.024</fontSize>
            <alignment>left</alignment>
        </text>
        <datetime name="md_lastplayed">
            <value>Never</value>
            <origin>0 0.5</origin>
